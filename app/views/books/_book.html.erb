<div id="<%= dom_id book %>">
  <p>
    <strong>Titulo:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Autor:</strong>
    <%= book.author %>
  </p>

  <p>
    <strong>Nota:</strong>
    <%= book.note %>
  </p>

  <p>
    <strong>Descrição:</strong>
    <%= book.description %>
  </p>

  <p>
    <strong>Sobre:</strong>
    <%= book.text %>
  </p>

</div>
